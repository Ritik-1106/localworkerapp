<h2>Your Job Applications</h2>

<table>
  <thead>
    <tr>
      
      <th>Description</th>
      <th>Status</th>
      <th>Applied On</th>
      <th>Jobs</th>
    </tr>
  </thead>
  <tbody>
    <% @my_applications.each do |application| %>
      <tr>
         
        <td><%= application.description %></td>
        <td><%= application.status %></td>
        <td><%= application.created_at.strftime("%Y-%m-%d") %></td>
       <td><%= link_to 'View Job', job_path(application.job_id) %></td>

        
      </tr>
    <% end %>
  </tbody>
</table>
